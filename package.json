{
  "name": "torrent2box",
  "version": "0.0.22",
  "description": "Upload torrents from your browser to your rutorrent seedbox.",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run check && npm run pack",
    "check": "vue-tsc --noEmit",
    "lint": "eslint --config .eslintrc.cjs --ext .ts,.vue ./src",
    "pack": "cross-env-shell NODE_ENV=production NODE_OPTIONS=\"--loader ./loader.mjs\" webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peardropsoftware/torrent2box.git"
  },
  "keywords": [],
  "author": {
    "name": "Rob Muchall",
    "email": "rmuchall@protonmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peardropsoftware/torrent2box/issues"
  },
  "homepage": "https://github.com/peardropsoftware/torrent2box#readme",
  "dependencies": {
    "bencode": "2.0.3",
    "buffer": "6.0.3",
    "fetch-hooks-ts": "0.0.8",
    "http-status-ts": "2.0.1",
    "meta-validator": "2.0.2",
    "sentence-case": "3.0.4",
    "vue": "3.3.4"
  },
  "devDependencies": {
    "@types/bencode": "2.0.1",
    "@types/chrome": "0.0.243",
    "@types/node": "20.4.8",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "css-loader": "6.8.1",
    "eslint": "8.46.0",
    "eslint-plugin-vue": "9.16.1",
    "html-loader": "4.2.0",
    "mini-css-extract-plugin": "2.7.6",
    "postcss": "8.4.27",
    "postcss-import": "15.1.0",
    "postcss-loader": "7.3.3",
    "tailwindcss": "3.3.3",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6",
    "vue-loader": "17.2.2",
    "vue-tsc": "1.8.8",
    "webpack": "5.88.2",
